package projet;

import java.time.LocalDate;
import java.time.LocalTime;

public class RendezVous {
	private LocalDate date2;
    private LocalTime heure2;

    public RendezVous(LocalDate  date2, LocalTime heure2) {
        this.date2 = date2;
        this.heure2 = heure2;
    }

    public LocalDate getDate() {
        return date2;
    }

    public void setDate(LocalDate date2) {
        this.date2 = date2;
    }

    public LocalTime getHeure() {
        return heure2;
    }

    public void setHeure(LocalTime heure2) {
        this.heure2 = heure2;
    }

    
    

}
